<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="oleh" id="1">
<!-- Liquibase doesn't support 'dataType' attribute for mssql, so it's is removed comparing to basic changeLog-->
<!-- see more on https://docs.liquibase.com/change-types/community/create-sequence.html-->
        <createSequence incrementBy="1"
                        minValue="1"
                        sequenceName="test_sequence"
                        startValue="1"/>
        <dropSequence sequenceName="test_sequence"/>
        <rollback/>
    </changeSet>
</databaseChangeLog>
