<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
        <!--https://docs.liquibase.com/change-types/create-procedure.html-->
    <changeSet author="as" id="1">
        <createProcedure
                comments="test procedure"
                dbms="postgresql"
                encoding="UTF-8"
                procedureName="test_procedure">CREATE OR REPLACE PROCEDURE test_procedure()
                                               LANGUAGE 'plpgsql'
                                               AS $$
                                               BEGIN
                                               END;
                                               $$
        </createProcedure>
        <rollback>
            <dropProcedure procedureName="test_procedure"/>
        </rollback>
    </changeSet>
</databaseChangeLog>