<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-next.xsd">

    <changeSet id="1" author="example">
        <createTable tableName="test_table">
            <column name="autoinc_test" type="int"/>
        </createTable>
    </changeSet>

    <changeSet id="2" author="example">
        <addAutoIncrement  tableName="test_table" columnName="autoinc_test" incrementBy="2" startWith="10"/>
        <rollback/>
    </changeSet>

</databaseChangeLog>
