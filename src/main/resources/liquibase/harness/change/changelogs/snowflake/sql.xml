<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    
    <!-- Include init.xml for cleanup -->
    <include file="liquibase/harness/change/changelogs/snowflake/init.xml"/>

    <changeSet id="1" author="kristyl">
        <comment>Creates a table and inserts values into the table with actual SQL</comment>
        <createTable tableName="sqltest">
            <column name="id" type="int"/>
        </createTable>
        <insert tableName="sqltest">
            <column name="id" valueNumeric="1"/>
        </insert>
        <insert tableName="sqltest">
            <column name="id" valueNumeric="2"/>
        </insert>
        <insert tableName="sqltest">
            <column name="id" valueNumeric="3"/>
        </insert>
        <rollback>
            <dropTable tableName="sqltest"/>
        </rollback>
    </changeSet>
</databaseChangeLog>