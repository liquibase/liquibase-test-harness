<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xmlns:pro-snowflake="http://www.liquibase.org/xml/ns/pro-snowflake"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd
        http://www.liquibase.org/xml/ns/pro-snowflake http://www.liquibase.org/xml/ns/pro-snowflake/liquibase-pro-snowflake-1.0.xsd">

    <changeSet author="oleh" id="1">
        <pro-snowflake:createFileFormat fileFormatName="ItTestFileFormat"
                                        replaceIfExists="false"
                                        alterIfExists="false"
                                        ifNotExists="true"
                                        temporary="false"
                                        comment="myComment"
                                        fileFormatType="CSV">
            <pro-snowflake:formatTypeOptions>
                <pro-snowflake:entry key="COMPRESSION" value="ZSTD"/>
                <pro-snowflake:entry key="BINARY_FORMAT" value="BASE64"/>
            </pro-snowflake:formatTypeOptions>
        </pro-snowflake:createFileFormat>
    </changeSet>
</databaseChangeLog>
